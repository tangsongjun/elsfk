r speed.</string><string name="timeLabel">Time:</string><string name="quitButtonString">Exit</string><string name="pref_help_resumability_summary">Under which conditions is an interrupted game resumable?</string><string name="pref_help_scoring_summary">How does the scoring work?</string><string name="pref_help_levels_summary">What do Levels mean?</string><string name="pref_vibDurOffset_title">Button Vibration Duration</string><string name="pref_help_levels_title">Levels</string><string name="action_help">Help</string><string name="score_title">Score</string><string name="pref_help_fps_title">FPS</string><string name="action_settings">Settings</string><string name="soundvolume_pref_title">Effects Volume</string><string name="pref_license_music_title">Music Attribution</string><string-array name="randomizer_preference_values">
